            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".menu.searching_teams.SearchTeam"
            android:label="@string/title_activity_search_team"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".now_registrate.TeamData" />
        <activity android:name=".now_registrate.SettingsActivity" />
        <activity android:name=".menu.NowRegActivity" />
        <activity
            android:name=".start_activities.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".start_activities.LoginActivity" />
        <activity
            android:name=".start_activities.MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>